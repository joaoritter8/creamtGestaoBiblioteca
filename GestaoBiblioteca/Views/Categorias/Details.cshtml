@model TesteBiblioteca.Models.Categoria

@{
    ViewData["Title"] = "Details";
}

<h1>Detalhes</h1>

<div>
    <h4>Categoria</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            NOME
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CatNome)
        </dd>
        <dt class = "col-sm-2">
            STATUS
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CatStatus)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.CatID">Editar</a> |
    <a asp-action="Index">Voltar</a>
</div>
